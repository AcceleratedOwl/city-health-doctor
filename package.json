{
  "name": "citypulse",
  "version": "1.0.0",
  "description": "CityPulse - Urban Health Monitor for NASA Space Apps Challenge 2025",
  "private": true,
  "dependencies": {
    "@types/leaflet": "^1.9.8",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "axios": "^1.6.2",
    "leaflet": "^1.9.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:production": "node scripts/build-production.js",
    "test": "react-scripts test",
